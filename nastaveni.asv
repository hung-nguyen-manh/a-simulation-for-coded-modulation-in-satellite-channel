    prokladanion = bitovyproklad0;
    shlukon = shlukon0;
    MAXglobal = MAX;
    MINglobal = MIN;
    Nramcu = Nramcu;
    simulacesbch = simulacesbch0;
    if shlukon == 1
        SNRdb = 100;
    end
    close;
end





