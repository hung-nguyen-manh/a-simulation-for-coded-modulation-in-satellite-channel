% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
global simulacesbch0 simulacesbch MAX MIN MAXglobal MINglobal MAX MIN prokladanion shlukon bitovyproklad0 shlukon0
global SNRdb Nramcu

MIN=get(handles.edit1,'String');
MIN = str2num(MIN);
MAX=get(handles.edit2,'String');
MAX = str2num(MAX);
Nramcu = get(handles.edit3,'String');
Nramcu = str2num(Nramcu);

if isempty(MAX) || isempty(MIN) || isempty(Nramcu)
    msgboxText{1} = ('Chybný formát pole');
    msgbox(msgboxText,'Chybný formát pole','error');
